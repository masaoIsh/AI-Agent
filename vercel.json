{
  "version": 2,
  "builds": [
    { "src": "api/sector.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/sector.py" },
    { "src": "/", "dest": "/web/index.html" },
    { "src": "/(.*\\.(css|js|png|jpg|svg|ico))", "dest": "/web/$1" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}


